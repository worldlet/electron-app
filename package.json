{
  "name": "test-electron",
  "version": "0.0.1",
  "author": "xuanzi",
  "description": "electron项目练习",
  "repository": "https://xuanzi.blog",
  "license": "MIT",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "electron .",
    "build-w": "electron-builder --config=./electron/builder.json",
    "build-w-32": "electron-builder --config=./electron/builder.json -w=nsis --ia32",
    "build-w-64": "electron-builder --config=./electron/builder.json -w=nsis --x64",
    "build-mac": "electron-builder --config=./electron/builder.json -m",
    "build-mac-arm64": "electron-builder --config=./electron/builder.json -m --arm64",
    "build-l": "electron-builder --config=./electron/builder.json -l=deb --x64",
    "build-l-32": "electron-builder --config=./electron/builder.json -l=deb --ia32",
    "build-l-64": "electron-builder --config=./electron/builder.json -l=deb --x64",
    "build-icon": "electron-icon-builder --input=./public/icon.png --output=build --flatten"
  },
  "dependencies": {
    "pinia": "^2.1.7",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5",
    "font-list": "^1.5.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.2",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "electron-icon-builder": "^2.0.1",
    "sass": "^1.69.6",
    "vite": "^5.0.10",
    "element-plus": "^2.4.4",
    "unplugin-auto-import": "^0.17.2",
    "unplugin-vue-components": "^0.26.0"
  }
}
